{
    "id": "mySkillsInfo",
    "title": "Show off your skills",
    "sections": ["skillProfile"],
    "breadCrumbs": [
        { "URL": "https://miles.univariety.com/alumni/dashboard", "Name": "My Dashboard" },
        { "URL": "https://miles.univariety.com/alumni/myprofile", "Name": "Profile" }
    ],
    "sectionRepository": [
        {
            "id": "skillProfile",
            "type": "SECTION_WITHOUT_HEADER",
            "className": "col-md-12",
            "controlGroup": [
                { "id": "resume", "type": "FILE_UPLOAD", "props": { "label": "Resume" }, "className": "col-md-12", "controlGroup": [] },
                {
                    "id": "skills",
                    "type": "MULTI_SELECT_WAI_3",
                    "props": { "label": "Mention up to 5 skills (Apologies for only 5)", "className": "col-md-12" },
                    "className": "col-md-12",
                    "controlGroup": [
                        { "id": "skillCode", "type": "SELECT", "props": { "domainCategoryCode": "SKILL_CODE" }, "className": "col-md-12" },
                        {
                            "id": "skillPreferenceCode",
                            "type": "RADIO",
                            "props": { "domainCategoryCode": "SKILL_PREFERENCE_CODE" },
                            "className": "col-md-12"
                        }
                    ]
                },
                {
                    "id": "interestedInUpskillingCode",
                    "type": "RADIO",
                    "props": { "label": "Interested in upskilling?", "domainCategoryCode": "INTERESTED_IN_UPSKILLING_CODE" },
                    "controlGroup": []
                },
                {
                    "id": "skillToTarget",
                    "type": "MULTI_SELECT_WAI_3",
                    "props": { "label": "Which skills would you want to target", "className": "col-md-12" },
                    "className": "col-md-12",
                    "controlGroup": [
                        { "id": "skillCode", "type": "SELECT", "props": { "domainCategoryCode": "SKILL_CODE" }, "className": "col-md-12" }
                    ]
                }
            ]
        }
    ]
}
